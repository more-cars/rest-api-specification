    "key": "MCA-551",
    "fields": {
      "summary": "Read-only properties cannot be overridden by the user when creating a COMPANY",
      "description": {
        "type": "doc",
        "version": 1,
        "content": [
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "text": "Read-only properties are for example `ID`, `created_at` and `updated_at`. Analog to unknown properties, those fields will be ignored. They will not be processed and there will be no info, warning or error that they were skipped."
              }
            ]
          }
        ]
